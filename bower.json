{
    "name": "angular-toolbelt",
    "description": "Handy directives and services that can be easily incorporated into any AngularJS web project",
    "version": "0.0.2-SNAPSHOT",
    "dependencies": {
        "angular": "latest",
        "angular-sanitize": "latest",
        "font-awesome": "latest",
        "bootstrap": "latest",
        "angular-bootstrap": "latest"
    },
    "devDependencies": {
        "angular-mocks": "latest"
    },
    "keywords": [
        "js",
        "angular",
        "scroll",
        "growl",
        "web"
    ],
    "homepage": "http://toolbelt.sysen.co.uk",
    "main": [
        "dist/**.*"
    ],
    "ignore": [
        "README.md",
        "Procfile",
        "**/.*",
        "*.js",
        "*.json",
        "node_modules",
        "bower_modules",
        "example",
        "lib",
        "logs",
        "src",
        "test"
    ],
    "exportsOverride": {
        "jquery": {},
        "bootstrap": {
            "css": "dist/css/bootstrap.min.css",
            "fonts": "fonts/*"
        },
        "font-awesome": {
            "css": "css/font-awesome.min.css",
            "fonts": "fonts/*"
        }
    }
}