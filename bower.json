{
    "name": "angular-toolbelt",
    "version": "0.0.1",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_modules",
        "test",
        "tests"
    ],
    "dependencies": {
        "angular": "latest",
        "angular-sanitize": "latest",
        "font-awesome": "latest",
        "bootstrap": "latest",
        "angular-bootstrap": "latest"
    },
    "devDependencies": {
        "angular-mocks": "latest"
    },
    "exportsOverride": {
        "jquery": {},
        "bootstrap": {
            "css": "dist/css/bootstrap.min.css",
            "fonts": "fonts/*"
        },
        "font-awesome": {
            "css": "css/font-awesome.min.css",
            "fonts": "fonts/*"
        }
    }
}